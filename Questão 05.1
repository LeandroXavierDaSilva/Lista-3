#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define TAM 100

int main(){
    char str1[TAM], str2[TAM];
    
    printf("Digite a primeira string: ");
    scanf("%s", str1);
    
    printf("Digite a segunda string: ");
    scanf("%s", str2);
    
    // Versão com uso da função strcat()
    strcat(str1, str2);
    printf("Versão com strcat(): %s\n", str1);
    
    return 0;
}
